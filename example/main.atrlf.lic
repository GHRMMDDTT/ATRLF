package example;

function Lexer:main(): = {
    whitespace() (number() | operator())
};

function Lexer:whitespace(): = {
    (' ' | '\t' | '\r' | '\n')?+
};

function Lexer:number(): = {
	<DigitLiteralToken : '1'..'9' '0'..'9'?+ ( '.' '0'..'9'?+ )? >
};

function Lexer:operator(): = {
	<PlusOperatorSymbolToken : '+'> |
	<MinusOperatorSymbolToken : '-'> |
    <StartOperatorSymbolToken : '*'> |
    <SlashOperatorSymbolToken : '/'>
};